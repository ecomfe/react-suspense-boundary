{
  "name": "react-suspense-boundary",
  "version": "1.1.0-beta.1",
  "repository": "git@github.com:ecomfe/react-suspense-boundary.git",
  "license": "MIT",
  "author": "otakustay <otakustay@gmail.com>",
  "files": [
    "cjs",
    "es"
  ],
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "build": "rm -rf es cjs && tsc & tsc --module ESNext --outDir ./es",
    "build:demo": "skr build --clean --src=demo",
    "lint": "skr lint src demo *.js",
    "lint-staged": "npm run lint -- --staged",
    "prepublishOnly": "npm run build",
    "deploy": "npm publish --registry=https://registry.npmjs.com",
    "start": "skr dev --src=demo",
    "test": "skr test --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "dependencies": {
    "@huse/derived-state": "^1.0.0",
    "@huse/update": "^1.0.0",
    "fast-json-stable-stringify": "^2.0.0",
    "prop-types": "^15.7.2",
    "tiny-invariant": "^1.1.0"
  },
  "devDependencies": {
    "@types/fast-json-stable-stringify": "^2.0.0",
    "@types/react": "^16.9.23",
    "antd": "^4.0.2",
    "echarts": "^4.4.0",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "prism-themes": "^1.3.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
    "react-loading": "^2.0.3",
    "react-markdown": "^4.2.2",
    "react-refractor": "^1.0.1",
    "react-router-dom": "^5.1.2",
    "refractor": "^2.10.0",
    "reskript": "^0.25.17",
    "webpack": "^4.42.0"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  }
}
